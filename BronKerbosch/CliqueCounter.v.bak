module CliqueCounter (
  input [2:0][2:0] graph,
  output reg [2:0] clique_count
);

always_comb begin
  clique_count = 0;
  for (int i = 0; i < 8; i = i + 1) begin
    if ((graph[i[2]][i[1]] && graph[i[1]][i[0]] && graph[i[2]][i[0]]) == 1)
      clique_count = clique_count + 1;
  end
end

endmodule
